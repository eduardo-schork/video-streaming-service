{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./build",
    "rootDir": "./",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@constants/*": ["src/constants/*"],
      "@features/*": ["src/features/*"],
      "@infra/*": ["src/infra/*"],
      "@shared/*": ["src/shared/*"],
      "@utils/*": ["src/utils/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
