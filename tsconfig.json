{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@infra/*": ["src/infra/*"],
      "@features/*": ["src/features/*"],
      "@core/*": ["src/core/*"],
      "@utils/*": ["src/utils/*"],
      "@constants/*": ["src/constants/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
